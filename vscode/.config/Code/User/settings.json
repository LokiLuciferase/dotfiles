{
    "[python]": {
        "editor.defaultFormatter": "charliermarsh.ruff"
    },
    "[typescript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[vue]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "database-client.telemetry.usesOnlineServices": false,
    "editor.cursorBlinking": "solid",
    "editor.formatOnSave": true,
    "editor.inlineSuggest.enabled": true,
    "editor.rulers": [
        120
    ],
    "eslint.format.enable": true,
    "explorer.confirmDelete": false,
    "files.insertFinalNewline": true,
    "gitlens.telemetry.enabled": false,
    "jupyter.askForKernelRestart": false,
    "notebook.formatOnSave.enabled": true,
    "playwright.reuseBrowser": true,
    "playwright.showTrace": false,
    "search.useGlobalIgnoreFiles": true,
    "security.promptForLocalFileProtocolHandling": false,
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.showExitAlert": false,
    "vim.easymotion": true,
    "vim.highlightedyank.color": "rgba(250, 240, 170, 0.3)",
    "vim.highlightedyank.duration": 100,
    "vim.highlightedyank.enable": true,
    "vim.leader": "<space>",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "1"
            ],
            "commands": [
                "workbench.action.openEditorAtIndex1"
            ]
        },
        {
            "before": [
                "<leader>",
                "2"
            ],
            "commands": [
                "workbench.action.openEditorAtIndex2"
            ]
        },
        {
            "before": [
                "<leader>",
                "3"
            ],
            "commands": [
                "workbench.action.openEditorAtIndex3"
            ]
        },
        {
            "before": [
                "<leader>",
                "4"
            ],
            "commands": [
                "workbench.action.openEditorAtIndex4"
            ]
        },
        {
            "before": [
                "<leader>",
                "5"
            ],
            "commands": [
                "workbench.action.openEditorAtIndex5"
            ]
        },
        {
            "before": [
                "<leader>",
                "6"
            ],
            "commands": [
                "workbench.action.openEditorAtIndex6"
            ]
        },
        {
            "before": [
                "<leader>",
                "7"
            ],
            "commands": [
                "workbench.action.openEditorAtIndex7"
            ]
        },
        {
            "before": [
                "<leader>",
                "8"
            ],
            "commands": [
                "workbench.action.openEditorAtIndex8"
            ]
        },
        {
            "before": [
                "<leader>",
                "9"
            ],
            "commands": [
                "workbench.action.openEditorAtIndex9"
            ]
        },
        {
            "before": [
                "<leader>",
                "f",
                "f"
            ],
            "commands": [
                "workbench.action.findInFiles"
            ]
        },
        {
            "before": [
                "<leader>",
                "l",
                "f"
            ],
            "commands": [
                "editor.action.formatDocument"
            ]
        },
        {
            "before": [
                "<leader>",
                "x",
                "d",
                "w"
            ],
            "commands": [
                "editor.action.trimTrailingWhitespace"
            ]
        },
        {
            "before": [
                "<leader>",
                "t",
                "t"
            ],
            "commands": [
                "workbench.action.terminal.toggleTerminal"
            ]
        },
        {
            "before": [
                "<leader>",
                "n",
                "c"
            ],
            "commands": [
                "testing.runAtCursor"
            ]
        },
        {
            "before": [
                "<leader>",
                "n",
                "r"
            ],
            "commands": [
                "testing.refreshTests"
            ]
        },
        {
            "before": [
                "q"
            ],
            "commands": [
                "workbench.action.closeActiveEditor"
            ]
        },
        {
            "before": [
                "K"
            ],
            "commands": [
                "editor.action.showHover"
            ]
        },
        {
            "before": [
                "<leader>",
                "l",
                "r"
            ],
            "commands": [
                "editor.action.rename"
            ]
        },
        {
            "before": [
                "<leader>",
                "l",
                "s",
                "o"
            ],
            "commands": [
                "outline.toggleVisibility"
            ]
        },
        {
            "before": [
                "<leader>",
                "l",
                "n"
            ],
            "commands": [
                "editor.action.marker.nextInFiles"
            ]
        },
        {
            "before": [
                "<leader>",
                "l",
                "p"
            ],
            "commands": [
                "editor.action.marker.prevInFiles"
            ]
        },
        {
            "before": [
                "<leader>",
                "c",
                "l"
            ],
            "commands": [
                "editor.action.commentLine"
            ]
        }
    ],
    "vim.smartRelativeLine": true,
    "vim.useSystemClipboard": true,
    "vim.vimrc.enable": true,
    "vim.vimrc.path": "$HOME/.config/nvim/01_vi.vim",
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "c",
                "l"
            ],
            "commands": [
                "editor.action.commentLine"
            ]
        }
    ],
    "window.menuBarVisibility": "toggle",
    "workbench.colorCustomizations": {
        "editorStickyScroll.background": "#31353d"
    },
    "workbench.colorTheme": "One Dark Pro Darker",
    "workbench.startupEditor": "none",
    "workbench.tree.indent": 32
}
