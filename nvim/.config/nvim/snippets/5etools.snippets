snippet sec5e "5e.tools section"
	{
		"type": "section",
		"name": "${1:section name}",
		"entries": [
			"$0"
		]
	}

snippet inset5e "5e.tools inset"
	{
		"type": "inset",
		"name": "${1:inset name}",
		"entries": [
			"$0"
		]
	}

snippet list5e "5e.tools list"
	{
		"type": "list",
		"items": [
			"$0"
		]
	}
snippet class "Full class skeleton, many of the fields do not always apply so delete those that are unused."
{
	"name": "$1",
	"source": "$2",
	"classFeatures": [
		$0
	],
	"hd": {
		"number": ${3:0},
		"faces": ${4:0}
	},
	"proficiency": [
		"${5|str,dex,con,int,wis,cha|}",
		"${6|str,dex,con,int,wis,cha|}"
	],
	"startingProficiencies": {
		"armor": [
			"${7|light,medium,heavy,{@item shield\|phb\|shields}|}"
		],
		"weapons": [
			"${8:simple and martial are rendered as x weapons, but you can write any string that you want.}"
		],
		"tools": [
			"${9:Name of any tool or artisan tools}"
		],
		"skills": [
			{
				"${10:skillName}": true
			}
		]
	},
	"startingEquipment": {
		"default": [
			"${11:Each string will appear as a list entry}"
		],
		"additionalFromBackground": true,
		"goldAlternative": "{@dice ${12:xdy}}"
	},
	"multiclassing": {
		"requirements": {
				"${13|str,dex,con,int,wis,cha|}": ${14:0}
			},
		"proficienciesGained": {
			"armor": [
				""
			],
			"weapons": [
				""
			],
			"tools": [
				""
			],
			"skills": [
				""
			]
		}
	},
	"casterProgression": "${15|full,1/2,1/3,pact|}",
	"spellcastingAbility": "${16|str,dex,con,int,wis,cha|}",
	"classSpells": [
		"$17"
	],
	"fluff": [
		${18}
	]
}
endsnippet

snippet classFeature "Skeleton of class feature"
{
	"name": "$1",
	"source": "$2",
	"className": "$3",
	"classSource": "$4",
	"page": ${5:0},
	"level": ${6:0},
	"entries": [
		"$7"
	]
}
endsnippet

snippet classFeatureRef "Reference a class feature inside a class"
"${1:<name>}|${2:<className>}|${3:<classSource>}|${4:<featureLevel>}|${5:<featureSource>}"
endsnippet

snippet subclass "Full subclass skeleton, delete the parts that don't apply to your case"
{
	"name": "$1",
	"source": "$2",
	"page": ${3:0},
	"className": "$4",
	"classSource": "$5",
	"shortName": "$6",
	"subclassFeatures": [
		$0
	],
	"casterProgression": "${15|full,1/2,1/3,pact|}",
	"spellcastingAbility": "${16|str,dex,con,int,wis,cha|}",
	"subclassSpells": [
		"$17"
	]
}
endsnippet

snippet subclassFeature "Skeleton of subclass feature"
{
	"name": "$1",
	"source": "$2",
	"className": "$3",
	"classSource": "$4",
	"page": ${5:0},
	"subclassShortName": "$6",
	"subclassSource": "$7",
	"level": ${8:0},
	"header": ${9:2},
	"entries": [
		"$10"
	]
}
endsnippet

snippet subclassFeatureRef "Reference a subclass feature inside a subclass"
"${1:<name>}|${2:<className>}|${3:<classSource>}|${4:<subclassShortName>}|${5:<subclassSource>}|${6:<featureLevel>}|${7:<featureSource>}"
endsnippet

snippet refSubclassFeature "Reference a subclass feature in the entries of another feature"
{
	"type": "refSubclassFeature",
	"subclassFeature": "${1:<name>}|${2:<className>}|${3:<classSource>}|${4:<subclassShortName>}|${5:<subclassSource>}|${6:<featureLevel>}|${7:<featureSource>}"
}
endsnippet
snippet section "Section entry, creates a level -1 header"
{
	"type": "section",
	"name": "$1",
	"page": ${2:0},
	"entries": [
		"$TM_SELECTED_TEXT$3"
	]
}
endsnippet

snippet entries "Entries block, for basic text"
{
	"type": "entries",
	"name": "$1",
	"page": ${2:0},
	"entries": [
		"$TM_SELECTED_TEXT$3"
	]
}
endsnippet

snippet entriesHeadless "Headless entries block, for going down one header level."
{
	"type": "entries",
	"entries": [
		$TM_SELECTED_TEXT$0
	]
}
endsnippet

snippet quote "Quote block"
{
	"type": "quote",
	"entries": [
		"$TM_SELECTED_TEXT$1"
	],
	"by": "${2:This can also be an array}",
	"from": "$3"
}
endsnippet

snippet inset "Inset block"
{
	"type": "inset",
	"page": ${1:0},
	"name": "$2",
	"entries": [
		"$TM_SELECTED_TEXT$3"
	]
}
endsnippet

snippet insetReadaloud "Inset block, but on blue color"
{
	"type": "insetReadaloud",
	"page": ${1:0},
	"name": "$2",
	"entries": [
		"$TM_SELECTED_TEXT$3"
	]
}
endsnippet

snippet list "List with style options. Remove the style line for default one with bullet points"
{
	"type": "list",
	"style": "${1|list-hang,list-hang-notitle,list-no-bullets,list-decimal,list-lower-roman,list-upper-roman|}",
	"columns": ${2:0},
	"page": ${3:0},
	"name": "$4",
	"items": [
		"$TM_SELECTED_TEXT$5"
	]
}
endsnippet

snippet listItem "An item within a list"
{
	"type": "item",
	"name": "$1",
	"entries": [
		"$TM_SELECTED_TEXT$2"
	]
}
endsnippet

snippet tableEntry "Table block used within text entries"
{
	"type": "table",
	"caption": "${1:Optional Caption}",
	"page": ${2:0},
	"colLabels": [
		"$3",
		"$4"
	],
	"colStyles": [
		"col-$5 text-$6",
		"col-$7 text-"
	],
	"intro": [
		"${8:Optional intro entries.}"
	],
	"outro": [
		"${9:Optional outro entries.}"
	],
	"rows": [
		[
			"$10",
			"$11"
		],
		[
			"$12",
			"$13"
		]
	],
	"footnotes": [
		"${14:Optional Footer}"
	]
}
endsnippet

snippet abilityDC "Centered text with the format 'Name save DC = 8 + prof + mod'"
{
	"type": "abilityDc",
	"name": "$1",
	"attributes": [
		"${2|str,dex,con,int,wis,cha|}"
	]
}
endsnippet

snippet abilityAttackMod "Centered text with the format 'Name attack modifier = prof + mod'"
{
	"type": "abilityAttackMod",
	"name": "$1",
	"attributes": [
		"${2|str,dex,con,int,wis,cha|}"
	]
}
endsnippet

snippet abilityGeneric "Centered text with the format 'Name  = text + mod'"
{
	"type": "abilityGeneric",
	"name": "$1",
	"text": "$2",
	"attributes": [
		"${3|str,dex,con,int,wis,cha|}"
	]
}
endsnippet

snippet gallery "A gallery entry that can contain multiple images."
{
	"type": "gallery",
	"images": [
		$TM_SELECTED_TEXT$1
	]
}
endsnippet

snippet image "External image link"
{
	"type": "image",
	"href": {
		"type": "external",
		"url": "$1"
	},
	"title": "${2:Optional Title}",
	"credit": "${3:Optional Artist Credit}"
}
endsnippet

snippet statblock "Use in text entries to embed the referenced data. (Sub)class features need extra properties"
{
	"type": "statblock",
	"tag": "${1|spell,item,creature,condition,disease,status,background,race,optfeature,reward,feat,psionic,object,cult,boon,trap,hazard,deity,variantrule,charoption,vehicle,vehupgrade,class,action,language,classFeature,subclassFeature,table,recipe,skill,sense,deck,legroup|}",
	"source": "$2",
	"name": "$3",
	"page": ${4:0}${5:,
	"collapsed": true}
}
endsnippet

snippet statblockProp "Use in text entries to embed the referenced subclass"
{
	"type": "statblock",
	"prop": "${1|subclass|}",
	"source": "$2",
	"name": "${3:Subclass ShortName}",
	"className": "$4",
	"classSource": "$5",
	"page": ${6:0}${7:,
	"collapsed": true}
}
endsnippet
snippet meta "Full basic _meta"
"_meta": {
	"sources": [
		{
			"json": "$1",
			"abbreviation": "$2",
			"full": "$3",
			"authors": [
				"$4"
			],
			"convertedBy": [
				"$5"
			],
			"version": "$6",
			"color": "${7:ffffff}",
			"url": "$8",
			"dateReleased": "${9:yyyy-mm-dd}"
		}
	],
	"status": "${10|wip,deprecated,ready,invalid|}",
	"dateAdded": $CURRENT_SECONDS_UNIX,
	"dateLastModified": $CURRENT_SECONDS_UNIX
}
endsnippet

snippet unixDate "The number of seconds since the Unix epoch"
$CURRENT_SECONDS_UNIX
endsnippet

snippet $schema "The JSON Schema for 5eTools Homebrew"
