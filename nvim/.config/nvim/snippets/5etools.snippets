snippet @bold "Bold Text"
{@b $TM_SELECTED_TEXT}
endsnippet

snippet @italic "Italic Text"
{@i $TM_SELECTED_TEXT}
endsnippet

snippet @strike "Strikethrough Text"
{@s $TM_SELECTED_TEXT}
endsnippet

snippet @underline "Underline Text"
{@u $TM_SELECTED_TEXT}
endsnippet

snippet @unit "Unit text, for recipe scaling"
{@unit $TM_SELECTED_TEXT|${1:unitSingle}|${2:unitPlural}}
endsnippet

snippet @h "Renders as "Hit: ". Do not use any text in it as it will not show up."
{@h }
endsnippet

snippet @hit "Renders as "Hit: +n". "
{@hit $TM_SELECTED_TEXT}
endsnippet

snippet @dc "Renders as "DC text""
{@dc $TM_SELECTED_TEXT}
endsnippet

snippet @chance "Shows the probability of success"
{@chance ${1:% of success: 'a roll higher than 100 minus this value'}|$TM_SELECTED_TEXT|${2:rolled by name}|${3:on success text}|${4:on failure text}}
endsnippet

snippet @coinflip "Make a coin flip with custom face names/outcomes"
{@coinflip $TM_SELECTED_TEXT|${2:rolled by name}|${3:Heads (Success)}|${4:Tails (Failure)}}
endsnippet

snippet @damage "Rolls for damage, with an XdY format (can use arithmetic operators)"
{@damage $TM_SELECTED_TEXT}
endsnippet

snippet @dice "A general dice roll, with an XdY format (can use arithmetic operators)"
{@dice $TM_SELECTED_TEXT|${1:display text}|${2:rolled by name}}
endsnippet

snippet @note "Note text, renders italicised with a faded grey look"
{@note $TM_SELECTED_TEXT}
endsnippet

snippet @sense "Tags a sense type, to show an explanation on hover"
{@sense $TM_SELECTED_TEXT|${1:Source (MM for tremorsense)}}
endsnippet

snippet @skill "Tags a skill type, to show an explanation on hover"
{@skill $TM_SELECTED_TEXT|${1:Source}}
endsnippet

snippet @adventure "Tags an adventure, or a specific chapter/section of it"
{@adventure $TM_SELECTED_TEXT|${1:source}|${2:chapter}|${3:heading}}
endsnippet

snippet @book "Tags a book, or a specific chapter/section of it"
{@adventure $TM_SELECTED_TEXT|${1:source}|${2:chapter}|${3:heading}}
endsnippet

snippet @footnote "Adds an inline footnote to the text, to be shown on hover"
{@footnote $TM_SELECTED_TEXT|${1:Footnote text}|${2:optional reference}}
endsnippet

snippet @link "Converts a url into an external hyperlink, with an optional display text"
{@link ${1:optional text}|$TM_SELECTED_TEXT}
endsnippet

snippet @color "Colors the text using the chosen hex code"
{@color $TM_SELECTED_TEXT|${1:hex code}}
endsnippet

snippet @highlight "Highlights the text in yellow"
{@highlight $TM_SELECTED_TEXT}
endsnippet

snippet @code "Formats the text as monospaced font"
{@code $TM_SELECTED_TEXT}
endsnippet

snippet @area "Tags an area within a book/adventure. The modifiers are a combination of "x" for e[x]act text, removing the leading "area" prepended to the name, and "u" for [u]ppercase, capitalizing the prepended "area""
{@area $TM_SELECTED_TEXT|${1:entry id}|${2:modifiers}}
endsnippet

